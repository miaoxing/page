{
  "name": "miaoxing/page",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Page Plugin",
  "keywords": [
    "miaoxing",
    "page"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.0",
    "miaoxing/link-to": "^0.1.14"
  },
  "require-dev": {
    "miaoxing/dev": "^6.4.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Page\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Page\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev",
      "miaoxing/services": "miaoxing/services as 0.3.x-dev",
      "miaoxing/app": "miaoxing/app as 0.3.x-dev",
      "miaoxing/link-to": "miaoxing/link-to as 0.1.x-dev",
      "wei/wei": "twinh/wei as 0.10.x-dev"
    }
  }
}
